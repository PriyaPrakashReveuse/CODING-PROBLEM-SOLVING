#include <iostream>

using namespace std;

int main()
{
    int i,n,arr[100],num;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    cin>>num;
    for(i=0;i<n;i++)
    {
        if(arr[i]==num)
        {
            cout<<i;
        }
        else
        {
        return -1;
        }
    }

    return 0;
}
