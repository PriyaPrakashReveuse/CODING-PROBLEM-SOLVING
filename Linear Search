#include <iostream>

using namespace std;

int linearsearch(int arr[],int n, int num)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]==num)
        {
            cout<<i;
            break;
        }
    }
    return -1;

}

int main()
{
    int i,N,arr[100],NUM;
    cin>>N;
    for(i=0;i<N;i++)
    {
        cin>>arr[i];
    }
    cin>>NUM;
    cout<<linearsearch(arr,N,NUM)<<endl;
    
    return 0;
    
}

