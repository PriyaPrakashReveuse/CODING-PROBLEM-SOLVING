#include <iostream>
using namespace std;

int main()
{
    int i,n,arr[100];
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int count=1,j=0;
    for(i=0;i<n-1;i++)
    {
        if(arr[j]>arr[i+1])
        {
            continue;
        }
        j=i+1;
        count++;
            
    } 
    cout<<count;
    return 0;
}
