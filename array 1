#include <iostream>

using namespace std;
int main()
{
   int arr[100];
   int n,temp;
   cin>>n;
   
   for(int i=0;i<n;i++)
   {
       cin>>arr[i];
   }
   
   for(int j=0;j<n;j++)
   {
       if (arr[j] > arr[j+1])  
       {
            temp=arr[j];
            arr[j]=arr[j+1];
            arr[j+1]=temp;
       }
   }
   cout<<arr[n-2]<<endl;
   return 0;
}
