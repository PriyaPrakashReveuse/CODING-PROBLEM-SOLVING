#include <iostream>
using namespace std;

int main() {
    
    int t;
    cin >> t;
    while(t--)
    {
        int n,num=0;
        cin>>n>>num;
        int arr[n];
        for(int i = 0; i < n; i++)
        {
            cin >> arr[i];
        }
        
        int count=0;
        for(int i=0;i<n;i++)
        {
            if(arr[i]==num)
            {
                count++;
            }
        }
        cout<<count;
    
    }
	return 0;
}
