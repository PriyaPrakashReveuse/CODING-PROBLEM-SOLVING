#include<iostream>
using namespace std;

int main()
{
  int t;
  cin >> t;
  while(t--)
  {
    int n;
    cin >> n;
    int arr[n];
    for(int i = 0; i < n; i++)
    {
      cin >> arr[i];
    }
    int count1 = 0,count2=0;
    for(int i = 0; i < n; i++)
    {
      if(arr[i] == 0)
      {
        count1++;
      }
    
      if(arr[i] == 1)
      {
        count2++;
      }
    }
    
    for(int i = 0; i < n; i++)
    {
      if(i < count1)
      {
        arr[i] = 0;
      }
      else if((i<(count1+count2))&&(i>=count1))
      {
        arr[i] = 1;
      }
      else if(i>=(count1+count2))
      {
          arr[i]=2;
      }
    }
    for(int i = 0; i < n; i++)
     {
       cout << arr[i] << " ";
     } cout << endl;
  }
}
