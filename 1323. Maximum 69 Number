int maximum69Number (int num) 
    {
        string s=to_string(num);
        for(int i=0;i<s.size();i++)
        {
            if(s[i]=='6')
            {
                s[i]='9';
                //just change once & return the number
                return stoi(s);
            }
        }
        // if the number is already the greatest just return it without changing
        return stoi(s);
    }
