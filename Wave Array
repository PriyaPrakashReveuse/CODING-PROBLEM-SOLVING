#include <bits/stdc++.h>
using namespace std;

int main() {
int t;
cin>>t;
while(t--)
{
  int n,arr[n];
  cin>>n;
  for(int i=0;i<n;i++)
  {
      cin>>arr[i];
  }
  for(int i=0;i<n-1;i=i+2)
  {  
        //  swap(arr[i],arr[i+1]);
        int temp;
        temp = arr[i];
        arr[i] = arr[i+1];
        arr[i+1] = temp;
    }
    
   for(int i=0;i<n;i++)
   {
      cout<<arr[i]<<" ";
    }
  cout<<endl;
}
	return 0;
}
